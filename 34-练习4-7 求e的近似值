#include<stdio.h>
double fact(int a);
int main()
{
	int n;
	int i;
	double s=0;
	scanf("%d",&n);
	if(n>1000){
		printf("error");
	}
    //第一项看作0的阶乘，因此s=0，
	for(i=0;i<=n;i++){
		s+=1.0/fact(i);
	}
	printf("%.8f\n",s);
}
double fact(int a)
{
	int i;
	double result=1.0;
	for(i=1;i<=a;i++){
		result*=i;
	}
	return result;
}
