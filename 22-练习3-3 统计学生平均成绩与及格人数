#include <stdio.h>
int main()
{
	int i,n,count;
	double score,s;
	scanf("%d",&n);
        count=0;
	s=0;
	for(i=0;i<n;i++){
		scanf("%lf",&score);
		s+=score;
		if(score>=60){
			count++;
		}	
	}
	if(n==0){
		printf("average = 0.0\n");
		printf("count = 0");
	}else{
	printf("average = %.1f\n",s/n);
	printf("count = %d",count);
    }
	
	return 0;
}
//count用于统计个数，注意n=0的时候要单独讨论，不然测试点不给分
